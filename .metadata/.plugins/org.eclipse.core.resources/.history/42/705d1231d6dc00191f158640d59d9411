		Scanner scnr = new Scanner(System.in); int data; int numPeople = 0;
		
		System.out.printf("Would you like to fill out this survey? Restart program if 10 people already used current count: %d\n", numPeople);
		
		System.out.printf("How would you rate these topics on a scale from 1 being the least important to 10 being the most important?\n");
		
		String topics [] = new String [5];
		topics[0] = ("2020 Presidential Election\n");
